<?xml version="1.0" ?>
<robot name="My robot">
    <webots>
        <device reference="ds0" type="DistanceSensor">
            <ros>
                <topicName>/left_sensor</topicName>
                <alwaysOn>true</alwaysOn>
            </ros>
        </device>
        <device reference="ds1" type="DistanceSensor">
            <ros>
                <topicName>/right_sensor</topicName>
                <alwaysOn>true</alwaysOn>
            </ros>
        </device>
        <device reference="gps" type="GPS">
            <ros>
                <topicName>/gps</topicName>
                <alwaysOn>true</alwaysOn>
            </ros>
        </device>
        <device reference="compass" type="Compass">
            <ros>
                <topicName>/compass</topicName>
                <alwaysOn>true</alwaysOn>
            </ros>
        </device>
        <plugin type="swarm_prod_sim.my_robot_driver.MyRobotDriver" />

    </webots>
</robot>